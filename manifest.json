{
  "manifest_version": 3,
  "name": "横戈团队Webpack映射提取器",
  "version": "1.0",
  "description": "横戈团队开发，提取Webpack生成的JS映射文件并支持一键加载",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon256.png",
      "48": "icons/icon256.png",
      "128": "icons/icon256.png"
    }
  },
  "icons": {
    "16": "icons/icon256.png",
    "48": "icons/icon256.png",
    "128": "icons/icon256.png"
  }
}